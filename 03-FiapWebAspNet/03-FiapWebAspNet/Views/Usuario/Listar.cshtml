@model IList<Usuario>
@{
    ViewData["Title"] = "Listar";
}

<h1>Listar</h1>

<table class="table">
    <tr>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.nome</td>
            <td>@item.dtNascimento</td>
            <td>@item.email</td>
        </tr>
        
    }

</table>
